{
  "name": "NL-BocPayUniPlugin",
  "id": "NL-BocPayUniPlugin",
  "version": "1.0.0",
  "description": "中银支付原生插件",
  "_dp_type": "nativeplugin",
  "_dp_nativeplugin": {
    "android": {
      "plugins": [{
        "type": "module",
        "name": "NL-BocPayUniPlugin",
        "class": "com.newland.bocpay.BocPay"
      }],
      "hooksClass": "com.newland.bocpay.BocPayAppProxy",
      "integrateType": "aar",
      "dependencies": [
        "com.facebook.fresco:fresco:1.13.0",
        "com.facebook.fresco:animated-gif:1.13.0",
        "com.github.bumptech.glide:glide:4.9.0",
        "androidx.webkit:webkit:1.3.0",
        "androidx.localbroadcastmanager:localbroadcastmanager:1.0.0",
        "androidx.core:core:1.1.0",
        "androidx.fragment:fragment:1.1.0",
        "com.orhanobut:logger:2.2.0",
        "org.greenrobot:eventbus:3.1.1",
        "io.reactivex.rxjava2:rxjava:2.2.6",
        "io.reactivex.rxjava2:rxandroid:2.1.0",
        "com.squareup.okhttp3:okhttp:3.14.2",
        "com.squareup.retrofit2:retrofit:2.9.0",
        "com.squareup.retrofit2:converter-gson:2.9.0",
        "com.squareup.retrofit2:adapter-rxjava:2.9.0",
        "com.squareup.okio:okio:2.9.0",
        "com.squareup.okhttp3:logging-interceptor:4.9.0"
      ],
      "compile0ptions": {
        "sourceCompatibility": "1.8",
        "targetCompatibility": "1.8"
      },
      "abis": [
        "armeabi-v7a",
        "arm64-v8a"
      ],
      "minSdkVersion": "22",
      "permissions": []
    },
    "ios": {
      "plugins": [{
        "type": "module",
        "name": "NL-BocPayUniPlugin",
        "class": "BocPayModule"
      }],
      "frameworks": [
        "BocAasPaySDK.framework",
        "AlipaySDK.framework"
      ],
      "embedFrameworks": [
        "BocAasPaySDK.framework",
        "AlipaySDK.framework"
      ],
      "resources": [
        "BocAasIosSDK.bundle",
        "AlipaySDK.bundle"
      ],
      "integrateType": "framework",
      "deploymentTarget": "11.0"
    }
  }
}